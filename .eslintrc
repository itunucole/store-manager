{
  "extends":"airbnb",
  "parserOptions":{
    "ecmaVersion":6
  },
  "env": {
    "node": true,
    "browser": true,
    "es6": true,
    "jasmine": true,
    "mocha": true
  },
  "rules": {
    "comma-dangle": [
    "error",
    "never"
    ],
    "no-used-vars": [
    "error",
    {
    "vars": "local",
    "args": "none"
  }
    ]  
  }
}
